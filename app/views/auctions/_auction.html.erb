<tr>
  <td><%= link_to auction.description, auction %></td>    
  <td><%= auction.url %></td>
  <td><%= number_with_delimiter auction.total_bids %></td>
  <td><%= number_to_currency auction.current_price %></td>  
  <td><%= link_to 'Delete', auction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
</tr>